@model List<bbnl.Models.registrationmodel>
@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

@{
    ViewData["Title"] = "Reg Data";
    if (@HttpContextAccessor.HttpContext.Session.GetString("usertype") == "admin")
    {
        Layout = "../Shared/_adminLayout.cshtml";
    }
    else
    {
        Layout = "../Shared/_adminLayoutpsu.cshtml";
    }
}


<div class="header bg-primary pb-6">
    <div class="container-fluid">
        <div class="header-body">
            <div class="row align-items-center py-4">
                <div class="col-lg-6 col-7">
                    <h6 class="h2 text-white d-inline-block mb-0">Registration Data</h6>
                    <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                        <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                            <li class="breadcrumb-item"><a href="/"><i class="fas fa-home"></i></a></li>

                            <li class="breadcrumb-item active" aria-current="page">Registration Data</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid mt--6">
    <div class="row">
        <div class="col">
            <div class="card">
                <!-- Card header -->
                <div class="card-header border-0">
                    <h3 class="mb-0">Registration Data</h3>
                </div>
                <!-- Light table -->
                <div class="table-responsive">
                    <table class="table align-items-center table-flush">
                        <thead class="thead-light">
                            <tr>
                                <th>Sr. No</th>
                                <th>Company Name</th>
                                <th>State</th>
                                <th>District</th>
                                <th>Type</th>
                                <th>Licence No</th>
                                <th>Applied For</th>
                                <th colspan="5" style="text-align:center">File</th>
                            </tr>
                            <tr>
                                <th colspan="7"></th>
                                <th>DOT</th>
                                <th>GSTIN</th>
                                <th>POA</th>
                                <th>POI</th>
                                <th>Company</th>

                            </tr>
                        </thead>
                        <tbody class="list">
                            @{int i = 1;}
                            @foreach (var data in Model)
                            {
                            <tr>

                                <td>@(i++)</td>

                                <td>
                                    @data.companyname
                                </td>
                                <td>
                                    @data.states_name
                                </td>
                                <td>
                                    @data.districts_name
                                </td>
                                <td>
                                    @data.companytype
                                </td>
                                <td>
                                    @data.licenceno
                                </td>
                                <td>
                                    @data.servicegp
                                </td>
                                <td>
                                    @if (!string.IsNullOrEmpty(@data.dotfilepath))
                                    {
                                        <a href="~/@data.dotfilepath" target="_blank">View</a>

                                    }

                                </td>
                                <td>
                                    @if (!string.IsNullOrEmpty(@data.gstinfilepath))
                                    {
                                        <a href="~/@data.gstinfilepath" target="_blank"> View </a>

                                    }

                                </td>
                                <td>
                                    @if (!string.IsNullOrEmpty(@data.poafilepath))
                                    {
                                        <a href="@data.poafilepath" target="_blank"> View </a>

                                    }

                                </td>
                                <td>
                                    @if (!string.IsNullOrEmpty(@data.poifilepath))
                                    {
                                        <a href="~/@data.poifilepath" target="_blank"> View </a>

                                    }

                                </td>
                                <td>
                                    @if (!string.IsNullOrEmpty(@data.compdocfilepath))
                                    {
                                        <a href="~/@data.compdocfilepath" target="_blank"> View </a>

                                    }

                                </td>




                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!--Card footer-->
                @*<div class="card-footer py-4">
                        <nav aria-label="...">
                            <ul class="pagination justify-content-end mb-0">
                                <li class="page-item disabled">
                                    <a class="page-link" href="#" tabindex="-1">
                                        <i class="fas fa-angle-left"></i>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                </li>
                                <li class="page-item active">
                                    <a class="page-link" href="#">1</a>
                                </li>
                                <li class="page-item">
                                    <a class="page-link" href="#">2 <span class="sr-only">(current)</span></a>
                                </li>
                                <li class="page-item"><a class="page-link" href="#">3</a></li>
                                <li class="page-item">
                                    <a class="page-link" href="#">
                                        <i class="fas fa-angle-right"></i>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </li>
                            </ul>
                        </nav>
                    </div>*@
            </div>
        </div>
    </div>


</div>
